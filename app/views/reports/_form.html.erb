<% form_for @report do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :query %><br />
    <%= f.text_field :query %>
  </p>
  <p>
    <%= f.label :database_id %><br />
    <%= f.text_field :database_id %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
